<!DOCTYPE html>
<html ng-app="testApp">
<head>
  <title>BucketlistApp</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>
<div class="header">
   <div class="header-center">
       <a href="#/">Home</a>
       <a href="#/interestDetail" class="interestDetail">InterestDetail</a>
       <a href="#/interests" class="interestPage">Interests</a>
       <a href="#/addInterest" class="addInterest">Admin</a>
       <a href="#/userBucket" class="userBucket">Bucket</a>
       <a href="#/bucketDetail" class="userBucketDetail">Bucket Detail</a>
       <a href="#/bucketlistitem" class="bucketlistitem">Bucket list items</a>
       <% if current_user %>
        <%= link_to 'Logout', destroy_user_session_path, method: :delete =%>
        /
        <%= link_to 'Update User Info', edit_user_registration_path =%>
        <% else %>
        <%= link_to 'Login', new_user_session_path =%>
        /
        <%= link_to 'Register', new_user_registration_path =%>
        <br>
      <% end %>
   </div>
</div>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
<div class= 'row'>
<%= yield %>
</div>





</body>
</html>
